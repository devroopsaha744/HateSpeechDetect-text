    "def compute_metrics(p):\n",
    "    print(type(p))\n",
    "    pred, labels = p\n",
    "    pred = np.argmax(pred, axis=1)\n",
    "\n",
    "    clf_rep = classification_report(y_true=labels, y_pred=pred)\n",
    "\n",
    "    return {\"classification_report\": clf_rep}"
   ]
  },
